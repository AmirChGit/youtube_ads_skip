{
    "manifest_version": 2,
    "name": "YouTube skip ads",
    "description": "executes a skip ad routine if it detects any. you can manually trigger the skip routine by pressing Â² or the mouse wheel button",
    "version": "1.2",
    "permissions": [
      "activeTab",
      "https://www.youtube.com/*"
    ],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "content_scripts": [
      {
        "matches": ["https://www.youtube.com/*"],
        "js": ["content.js"]
      }
    ],
    "icons": {
      "48": "icon.png"
    }
  }
  